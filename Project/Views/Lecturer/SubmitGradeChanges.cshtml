@using Project.Models; 
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Edit Grades </title>
</head>
<body>
    <div>
        <center>
            <form method="post" action="SubmitGrades" onsubmit="SetData()">
                <select name="studentId" id="studentId">
                    @foreach (Student_Course_Model x in ViewBag.studentInCourse)
                    {
                        <option> @x.StudentID </option>
                    }
                </select>
                Grade = <input id="grade" name="grade" type="number"/> 
                <input type="submit" value="submit" />

            </form>
        </center>


    </div>
</body>
</html>
